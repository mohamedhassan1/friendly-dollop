#include <iostream>
#include <algorithm>
#include <string.h>
#include <string>
using namespace std;
string A, B;  
int C=0; 

void prem(string str) {
	int s = 0; int z = 0;
	if (z <str.size()) {
		
    
    
		for (int x = str.size()-1; x > 1; x--) {
			swap(str[x], str[x - 1]);
			if (str == B)break;
			cout << str << endl;
		}
	}
		
    
    
    
    if (str == B) {
			swap(B[0], B[C + 1]);
			C++;
			z++;
			str = B;
			if (C == str.size())return;
			cout << str << endl;
		}
		
		return prem(str);
}






int main()
{
	 cin>>A ;
	B = A;
	prem(A);
}
